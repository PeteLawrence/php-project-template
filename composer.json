{
    "name": "PeteLawrence/php-project-template",
    "description": "A starting point for PHP projects that want to make use of continuous integration tools.",
    "keywords": ["continuous", "integration", "jenkins", "phpunit", "vagrant"],
    "homepage": "https://github.com/PeteLawrence/php-project-template",
    "authors": [
        {
            "name": "Pete Lawrence",
            "email": "petejlawrence@icloud.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/PeteLawrence/php-project-template/issues",
        "source": "https://github.com/PeteLawrence/php-project-template"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*@dev",
        "phpunit/php-code-coverage": "2.0.*@dev",
        "phpunit/phpunit-mock-objects": "2.0.*@dev",
        "sebastian/version": "1.0.*",
        "sebastian/environment": "1.0.*@dev",
        "theseer/phpdox": "0.6.5",
        "sebastian/phpcpd": "2.0.*",
        "phpmd/phpmd": "dev-master",
        "pdepend/pdepend": "2.0.*",
        "phploc/phploc": "2.0.*@dev",
        "instaclick/php-code-sniffer": "dev-patch-3"
    },
    "autoload": {
        "psr-4": {"": "src/"}
    }
}
